<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Alfheim.GUI.Styles"
                    xmlns:m="clr-namespace:Alfheim.GUI.Controls"              
                    xmlns:lvc="clr-namespace:LiveCharts.Wpf;assembly=LiveCharts.Wpf">
    <Style
        x:Key="DefaultListBoxWithHeader"
        TargetType="m:ListBoxWithHeader">
        
        <Setter
             Property="Margin"
             Value="6 3" />
    </Style>

    <Style
        x:Key="DefaultPlot"
        TargetType="lvc:CartesianChart">

        <Setter
             Property="Margin"
             Value="6 3" />
    </Style>

    <Style
        x:Key="DefaultTextBlockWithTitle"
        TargetType="m:TextBlockWithTitle">

        <Setter
             Property="Margin"
             Value="6 3" />
    </Style>
</ResourceDictionary>